{
  "name": "hayamimaker",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://hayamimaker.web.app",
  "dependencies": {
    "@material-ui/core": "4.9.5",
    "@material-ui/icons": "4.9.1",
    "crypto-js": "4.0.0",
    "dotenv": "^8.2.0",
    "firebase": "8.0.2",
    "formik": "2.1.4",
    "history": "4.10.1",
    "lodash": "4.17.15",
    "moment": "2.24.0",
    "react": "16.13.0",
    "react-dom": "16.13.0",
    "react-helmet": "5.2.1",
    "react-redux": "7.2.0",
    "react-router-dom": "5.1.2",
    "redux": "4.0.5",
    "redux-persist": "6.0.0",
    "redux-thunk": "2.3.0",
    "styled-components": "5.0.1",
    "typescript-fsa": "3.0.0",
    "typescript-fsa-reducers": "1.2.1"
  },
  "devDependencies": {
    "@types/crypto-js": "3.1.43",
    "@types/jest": "25.1.3",
    "@types/lodash": "4.14.149",
    "@types/node": "^13.7.7",
    "@types/react": "16.9.23",
    "@types/react-dom": "16.9.5",
    "@types/react-helmet": "5.0.15",
    "@types/react-redux": "7.1.7",
    "@types/react-router-dom": "5.1.3",
    "@typescript-eslint/eslint-plugin": "2.22.0",
    "env-cmd": "^10.1.0",
    "eslint": "6.8.0",
    "eslint-config-nzap": "1.4.6",
    "eslint-config-prettier": "6.10.0",
    "eslint-config-react-app": "5.2.0",
    "eslint-plugin-jest": "23.8.1",
    "eslint-plugin-prettier": "3.1.2",
    "firebase-admin": "^8.9.2",
    "husky": "4.2.3",
    "lint-staged": "10.0.8",
    "prettier": "1.19.1",
    "qawolf": "0.9.3",
    "react-dev-utils": "10.2.0",
    "react-scripts": "3.4.0",
    "ts-node": "^8.10.1",
    "tslint": "6.0.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-plugin-prettier": "2.1.0",
    "tslint-react": "4.2.0",
    "typescript": "3.8.3"
  },
  "scripts": {
    "dev": "HOST=localhost.hayami react-scripts start",
    "build": "react-scripts build && \\rm -rf functions/lib/build && mv build functions/lib",
    "eject": "react-scripts eject",
    "deploy:production": "firebase use default && yarn run build && firebase deploy --only functions:filter && firebase use staging",
    "deploy:database": "firebase deploy --only database",
    "deploy:staging": "firebase use staging && env-cmd -f .env.development yarn run build && firebase deploy --only functions:filter",
    "test": "react-scripts test --env=jsdom",
    "deploy:firebase": "firebase deploy",
    "lint": "eslint --ext .ts,.tsx src"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --single-quote --write",
      "git add"
    ]
  }
}
